{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BlockchainSettings": {
    "EnableRegistration": true
  },
  "DatabaseSettings": {
    "AutoResetOnStartup": false,
    "AllowDatabaseAdminApi": true
  },
  "FrontendSettings": {
    "BaseUrl": "https://uapblockchain.vercel.app",
    "VerifyPath": "/certificates/verify",
    "DefaultQrSize": 300
  },
  "CloudinarySettings": {
    "ProfileImagesFolder": "profile-images"
  },
  "ValidationSettings": {
    "EnforceAttendanceDateValidation": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=FapDb;User Id=sa;Password=12345;TrustServerCertificate=True;Encrypt=False;"
  }
